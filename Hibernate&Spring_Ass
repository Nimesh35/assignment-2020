package a7;

public class Rate implements InterestRate {

	@Override
	public int getRate() {
		// TODO Auto-generated method stub
		return 600;
	}

}
--------------

SBI class

package a7;

import org.pro1.FortuneService;

public class SBI implements Bank {

	
	private InterestRate Is;

	
	
	
	public SBI(InterestRate is) {
		super();
		Is = is;
	}

	@Override
	public String Name() {
		// TODO Auto-generated method stub
		return "SBI BANK";
	}

	@Override
	public int Rate() {
		// TODO Auto-generated method stub
		
		return Is.getRate();
	}

	
	

}
