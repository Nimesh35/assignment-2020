public class Store implements Interface

{

	@Override
	public String status() 
	
	{
		
		return "COMPLETED";
	}
	
	@Override
	public String getusername() 
	
	{
		
		return "Nimesh";
	}
	
	@Override
	public String getemail() 
	
	{
		
		return "nimeshudayan3@gmail.com"
	}
	
	@Override
	public String getpassword() 
	
	{
		
		return "nimesh123
	}
